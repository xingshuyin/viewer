    "@vue/eslint-config-prettier": "^7.1.0",
    "electron": "^24.6.2",
    "electron-builder": "^23.6.0",
    "electron-vite": "^1.0.25",
    "eslint": "^8.44.0",
    "eslint-plugin-vue": "^9.15.1",
